pipeline {
    agent any
    
    stages {
        
        stage("build") {
            steps {
                echo 'this is build stage'
            }
        }
        stage("test") {
            
            steps {
		sleep(500)
                echo 'this is test stage'
                snDevOpsChange()
            }
        }
        stage("deploy") {
            steps {
                echo 'this is deploy stage'
            }
        }
        
    }   
}
